      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^5.1.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
