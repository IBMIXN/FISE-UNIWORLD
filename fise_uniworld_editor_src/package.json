  },
  "jest": {
    "moduleNameMapper": {
      "\\.worker.js": "<rootDir>/src/mockdata/workerMock.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/App.js",
